{
  "name": "creditcards",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.2",
    "@tanstack/react-query": "^4.0.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^18.6.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^0.27.2",
    "json-server": "^0.17.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@testing-library/user-event": "^14.4.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "concurrently": "^7.3.0",
    "cypress": "^10.4.0",
    "eslint": "^8.20.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3"
  },
  "scripts": {
    "start": "concurrently \"PORT=3001 react-scripts start\" \"json-server --watch db.json --port 3004 --delay 2000\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint:eslint:fix": "eslint --no-error-on-unmatched-pattern --fix --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "test:ci": "yarn test --watchAll=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "{cypress,src}/**/*.{ts,tsx,js,jsx}": [
      "yarn lint:eslint:fix",
      "git add --force"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add --force"
    ]
  }
}
